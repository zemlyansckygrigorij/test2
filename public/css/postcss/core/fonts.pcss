@font-face {
	font-family: 'Roboto-Condensed';
	src: url('/public/fonts/Roboto-Condensed/Roboto-Condensed.eot');
	src: url('/public/fonts/Roboto-Condensed/Roboto-Condensed.eot?#iefix') format('embedded-opentype'),
		url('/public/fonts/Roboto-Condensed/Roboto-Condensed.woff2') format('woff2'),
		url('/public/fonts/Roboto-Condensed/Roboto-Condensed.woff') format('woff'),
		url('/public/fonts/Roboto-Condensed/Roboto-Condensed.ttf') format('truetype');
	font-weight: 400;
	font-style: normal;
}

